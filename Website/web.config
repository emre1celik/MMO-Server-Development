<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<system.webServer>
        <directoryBrowse enabled="false" />
        <handlers>
            <add name="PHP" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="C:\PHP\php-cgi.exe" resourceType="File" />
        </handlers>
	</system.webServer>
</configuration>